{
  "version": 2,
  "builds": [
    {
      "src": "api/base-frame.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/game.html",
      "dest": "/public/game.html"
    },
    {
      "src": "/base-frame",
      "dest": "/public/base-frame.html"
    },
    {
      "src": "/api/base-frame",
      "dest": "/api/base-frame.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
